#include <iostream>
using namespace std;
#include<string>

int main() {
	int T;
	cin>>T;
	while(T--){
	    int n;
	    cin>>n;
	    string arr[n];
	    int c=0,p=0;
	    for(int i=0;i<n;i++){
	        cin>>arr[i];
	    }
	    for(int i=0;i<n;i++){
	        if(arr[i]=="START38"){
	            c++;
	        }
	        else{
	            p++;
	        }
	    
	    	
	    }
	    cout<<c<<" "<<p<<endl;
	    
	}

	return 0;
}
